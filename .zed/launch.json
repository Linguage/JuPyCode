    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File (henri_env)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "${command:python.interpreterPath}"
        },
        {
            "name": "Run Julia File",
            "type": "julia",
            "request": "launch",
            "program": "${file}",
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "args": []
        }
    ]
}